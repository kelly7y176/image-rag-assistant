# =========================================================
# 1. IMMICH CORE DATABASE CONFIG (Used by Immich Services)
# =========================================================
DB_USERNAME=postgres
DB_PASSWORD=your_immich_db_password_here
DB_DATABASE_NAME=immich
IMMICH_VERSION=v2.0.1
JWT_SECRET=a_very_long_secure_jwt_secret_here

# 設置 Immich 資料儲存位置
UPLOAD_LOCATION=/Users/fulam/immich-library/upload
DB_DATA_LOCATION=/Users/fulam/immich-app/postgres

# =========================================================
# 2. N8N RAG/VECTOR DB CONFIG (Used by n8n service)
# =========================================================
# n8n 服務連接到 pgvector_n8n 服務的憑證
N8N_DB_USER=n8n_user
N8N_DB_PASSWORD=n8n_password
N8N_DB_DATABASE=n8n_vector_db

# Google AI/OAuth 密鑰
GEMINI_API_KEY=YOUR_GEMINI_API_KEY_HERE
GOOGLE_OAUTH_CLIENT_ID=YOUR_GOOGLE_CLIENT_ID
GOOGLE_OAUTH_CLIENT_SECRET=YOUR_GOOGLE_CLIENT_SECRET

# n8n Webhook URL
N8N_HOST=localhost
N8N_PORT=5678
N8N_PROTOCOL=http
WEBHOOK_URL=http://localhost:5678/
